{
  "name": "app-forms",
  "version": "2.1.0",
  "description": "Forms management application",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development node_modules/.bin/webpack-dev-server --hot --config webpack.config.js --mode development",
    "build": "cross-env NODE_ENV=production node_modules/.bin/webpack --config webpack.config.js --progress --colors",
    "test": "cross-env NODE_ENV=development karma start",
    "pack": "cross-env NODE_ENV=production APP_HOST=app.forms.li node_modules/.bin/webpack",
    "pack:dev": "cross-env NODE_ENV=development APP_HOST=dev-app.forms.li node_modules/.bin/webpack",
    "sync:dev": "aws s3 sync dist/ s3://fl-dev-webapp --acl public-read --profile fl-infrastructure && aws s3 sync public/ s3://fl-dev-webapp/public --acl public-read --profile fl-infrastructure",
    "sync:prod": "aws s3 sync dist/ s3://fl-prod-webapp --acl public-read --profile fl-infrastructure && aws s3 sync public/ s3://fl-prod-webapp/public --acl public-read --profile fl-infrastructure",
    "deploy:dev": "cross-env NODE_ENV=development APP_HOST=dev-app.forms.li npm run pack:dev && npm run sync:dev",
    "deploy:prod": "cross-env NODE_ENV=production APP_HOST=app.forms.li npm run pack && npm run sync:prod",
    "preversion": "npm test",
    "version": "npm run build && git add -A lib",
    "postversion": "git push && git push --tags",
    "release": "npm version patch && npm publish",
    "release:minor": "npm version minor && npm publish",
    "release:major": "npm version major && npm publish"
  },
  "author": "Kartik Rao",
  "license": "UNLICENSED",
  "dependencies": {
    "@aws-amplify/analytics": "^1.2.20",
    "@aws-amplify/api": "^1.0.42",
    "@aws-amplify/auth": "^1.2.31",
    "@aws-amplify/core": "^1.1.0",
    "@aws-amplify/interactions": "^1.0.32",
    "@aws-amplify/pubsub": "^1.1.0",
    "@aws-amplify/storage": "^1.1.0",
    "@aws-amplify/ui": "^1.0.23",
    "@aws-amplify/xr": "^0.1.22",
    "@kartikrao/lib-forms": "^3.7.5",
    "@kartikrao/lib-forms-core": "^6.6.3",
    "@kartikrao/lib-logging": "^1.6.0",
    "@kartikrao/lib-storage": "^1.2.2",
    "@sentry/browser": "^4.6.4",
    "antd": "^3.23.2",
    "aws-amplify-react": "^2.4.2",
    "dayjs": "^1.8.16",
    "director": "^1.2.8",
    "glamor": "^2.20.40",
    "graphql-tag": "^2.10.1",
    "history": "^4.9.0",
    "jwt-decode": "^2.2.0",
    "mobx": "^5.9.4",
    "mobx-logger": "^0.7.1",
    "mobx-react": "^6.1.3",
    "mobx-utils": "^5.2.0",
    "moment-timezone": "^0.5.26",
    "qrcode.react": "^0.9.3",
    "query-string": "^6.2.0",
    "react": "^16.9.0",
    "react-beautiful-dnd": "^11.0.3",
    "react-dom": "^16.9.0",
    "react-highlight-words": "^0.16.0",
    "react-router-dom": "^5.0.1"
  },
  "devDependencies": {
    "@types/history": "^4.7.2",
    "@types/lodash": "^4.14.123",
    "@types/node": "^11.9.4",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "@types/react-router": "^5.0.3",
    "@types/react-router-dom": "^4.3.5",
    "airbnb-browser-shims": "^3.1.0",
    "awesome-typescript-loader": "^5.2.1",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.0",
    "fork-ts-checker-webpack-plugin": "^1.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^4.0.0-beta.8",
    "jasmine": "^3.3.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-jasmine": "^2.0.1",
    "karma-mocha-reporter": "^2.2.5",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^3.0.5",
    "mini-css-extract-plugin": "^0.8.0",
    "style-loader": "^0.23.1",
    "svg-inline-loader": "^0.8.0",
    "terser": "^3.14.1",
    "ts-import-plugin": "^1.5.5",
    "ts-loader": "^5.3.3",
    "typescript": "^3.6.3",
    "webpack": "^4.39.2",
    "webpack-bundle-analyzer": "^3.4.1",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.7.1"
  },
  "resolutions": {
